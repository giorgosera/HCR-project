<script type="text/javascript">
function autoplayoff(){
days=30;
myDate = new Date();
myDate.setTime(myDate.getTime()+(days*24*60*60*1000));
document.cookie = 'autoplayoff=true; expires=' + myDate.toGMTString();
}
</script>
<script type="text/javascript">
function autoplayon(){
days=30;
myDate = new Date();
myDate.setTime(myDate.getTime()-(days*24*60*60*1000));
document.cookie = 'autoplayoff=; expires=' + myDate.toGMTString();
}
</script>

